{
  "name": "kubos-linux-pumpkin-mbm2-gcc",
  "version": "0.1.1",
  "description": "Base KubOS Linux build target for the Pumpkin MBM2 arm-linux toolchain",
  "inherits": {
    "kubos-linux-gcc": "kubos/target-kubos-linux-gcc#~0.1.0"
  },
  "licenses": [
    {
      "url": "https://spdx.org/licenses/Apache-2.0",
      "type": "Apache-2.0"
    }
  ],
  "keywords": [
    "kubos-linux",
    "linux",
    "beaglebone-black",
    "pumpkin",
    "arm",
    "gcc"
  ],
  "buildTarget": true,
  "similarTo": [
    "linux",
    "kubos-linux",
    "arm-linux-gcc",
    "arm",
    "beaglebone-black",
    "pumpkin"
  ],
  "config": {
	"hardware": {
	  "i2c": {
	    "count": 1,
	    "defaults": {
	      "bus": "K_I2C1"
	    },
	    "i2c1": {
		  "device": "/dev/i2c-1"
	    }
	  }
	}
  },
  "toolchain": "CMake/toolchain.cmake"
}

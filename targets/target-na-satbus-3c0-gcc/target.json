{
  "name": "na-satbus-3c0-gcc",
  "version": "0.1.0",
  "inherits": {
    "stm32f405-gcc": "kubos/target-stm32f405-gcc#~0.1.0"
  },
  "description": "KubOS build target for the NanoAvionics SatBus 3C0 On-board Computer",
  "author": "Marshall Culpepper <marshall@kubos.co>",
  "homepage": "https://github.com/kubos/target-na-satbus-3c0-gcc",
  "licenses": [
    {
      "url": "https://spdx.org/licenses/Apache-2.0",
      "type": "Apache-2.0"
    }
  ],
  "keywords": [
    "na-satbus-3c0",
    "stm32f405rg",
    "gcc"
  ],
  "similarTo": [
    "na-satbus-3c0",
    "stm32f405rg",
    "f405rg",
    "f405",
    "stm32f405rg",
    "stm32f405",
    "stm32f4",
    "stm32",
    "st",
    "cortex-m4",
    "armv7-m"
  ],
  "buildTarget": true,
  "config": {
    "hardware": {
      "externalClock": "24000000",
      "console": {
        "uart": "K_UART6",
        "baudRate": "115200"
      },
      "pins": {
        "LED1": "PD12",
        "LED2": "PD13",
        "LED3": "PD14",
        "LED4": "PD15",
        "USER_BUTTON": "PA0"
      }
    }
  },
  "toolchain": "CMake/toolchain.cmake",
  "debug": [
    "valinor",
    "--target",
    "stm32f405rg",
    "$program"
  ],
  "scripts": {
    "debug": [
      "valinor",
      "--target",
      "stm32f405rg",
      "$program"
    ],
    "test": [
      "mbed_test_wrapper",
      "--target",
      "STM32F4-DISCO",
      "$program"
    ]
  }
}
